<% layout('layouts/boilerplate') -%>
    <!-- Navbar Start -->
    <div class="container-fluid bg-light position-relative shadow">
        <nav class="navbar navbar-expand-lg bg-light navbar-light  py-3 py-lg-0 px-0 px-lg-5">
            <a href="" class="navbar-brand font-weight-bold text-secondary" style="font-size: 50px;">
                <img src="/img/Logo of the Language Center at the University of Jordan.png "
                    style="width:70px; padding: 10px 10px;" alt="logo">

                <span class="text-primary">JUBest</span>
            </a>
            <button type="button" class="navbar-toggler" data-toggle="collapse" data-target="#navbarCollapse">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse justify-content-lg-end " id="navbarCollapse">
                <div class="navbar-nav font-weight-bold  py-0">
                    <a href="/" class="nav-item nav-link active ">Home</a>
                     

                </div>
                <% if(!currentUser) {%>
                    <a href="/signin" class="btn btn-primary px-4">Sign in</a>
                    <a href="/register" class="px-4">Sign up</a>
                    <% } else { %>
                        <a href="/levels" class="btn btn-outline-primary px-4">levels</a>
                        <button type="button" class="btn btn-primary mx-1 px-4" data-bs-toggle="modal"
                            data-bs-target="#logoutModal">
                            Sign out
                        </button>
                        <%- include('../partials/signout') %>
                            <% } %>
            </div>
        </nav>
    </div>
    <!-- Navbar End -->


    <!-- Header Start -->
    <div class="container-fluid bg-primary px-0 px-md-5 mb-5">
        <div class="row align-items-center px-3">
            <div class="col-lg-6 text-center text-lg-left">
                <h4 class="text-white mb-4 mt-5 mt-lg-0">JUBest Website</h4>
                <h1 class="display-3 font-weight-bold text-white">New Approach to Language Center University of Jordan
                </h1>
                <p class="text-white mb-4">Is a web platform that allow Non-Arabic speaker who went to learn Arabic
                    language in JU .</p>
                <a href="https://centers.ju.edu.jo/en/aqlc/Pages/ASOL-info.aspx"
                    class="btn btn-secondary mt-1 py-3 px-5">Learn More</a>
            </div>
            <div class="col-lg-6 text-center text-lg-right">
                <img class="img-fluid mt-5 rounded-circle img-thumbnail" src="/img/language center.jpg" alt="">
            </div>
        </div>
    </div>
    <!-- Header End -->


